{
    "name": "harout-itology/laravel-aws-timestream",
    "description": "Library to interact with AWS Timestream service via API",
    "keywords": ["aws timestream", "laravel aws timestream", "laravel timeseries db", "timeseries db"],
    "type": "library",
    "homepage": "https://github.com/harout-itology/laravel-aws-timestream",
    "license": "MIT",
    "extra": {
        "laravel": {
            "providers": [
                "HaroutItology\\AwsTimestream\\TimestreamServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "HaroutItology\\AwsTimestream\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HaroutItology\\AwsTimestream\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Norby Baruani",
            "email": "norby.baruani@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Harout ITology",
            "email": "harout.itology@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0",
        "aws/aws-sdk-php": "^3.209"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.4",
        "nunomaduro/larastan": "^1.0|^2.0",
        "orchestra/testbench": "^6.24|^7.0",
        "phpunit/phpunit": "^9.5",
        "vlucas/phpdotenv": "^5.4"
    }
}
